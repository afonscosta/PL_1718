set terminal png font "arial,10" size 800,800
set output './imagens/output.png'
HP1 = "#99ffff"; HP2 = "#4671d5"
set yrange [0:1500]
set style data histogram
set style histogram cluster gap 1
set style fill solid border -1
set boxwidth 0.9
set xtic scale 0
plot 'hp.dat' using 2:xtic(1) ti col fc rgb HP1, '' u 3 ti col fc rgb HP2
